<?xml version="1.0" encoding="UTF-8"?>
<lint>
    <!--
       IntelliJ Bugs
        "Element X is not allowed here" error message is no problem.
        https://code.google.com/p/android/issues/detail?id=175741
     -->

    <issue id="InvalidPackage">
        <!--
           Orma uses antlr4 to parse SQL, and antlr4-runtime imports java.awt.*, which causes a lint violation on InvalidPackage.
           https://github.com/gfx/Android-Orma#lint-fails-on-invalidpackage
         -->
        <ignore regexp="antlr4-runtime-4.5.1.jar" />

        <!--
           Stethoが依存するokhttpが内包するokio.
           https://github.com/square/okio/issues/58
         -->
        <ignore regexp="okio-1.6.0.jar" />
    </issue>
</lint>
